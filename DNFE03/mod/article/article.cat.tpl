<table class="cats">
     <tr class="cat-head">
         <td>{all_icon}</td><td>{cd}</td><td class="ac">{all_col}</td>
     </tr>
     {artprint}
     <!--buffer:rows:0-->
     <tr class="cat-body">
         <td class="ac wpc_5">{icon}</td>
         <td class="al wpc_80">{title}{desc}<br />{sub}</td>
         <td class="ac va wpc_15">{total}</td>
    </tr>
    <!--buffer-->
    <tr class="cat-info">
        <td colspan="3" class="ar"><em>{all_alls}: {alltotal}, {all_cats}: {allcat}, {article_hits}: {allhits}</em></td>
    </tr> 
</table>
